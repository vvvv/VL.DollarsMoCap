<?xml version="1.0"?>

<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>VL.DollarsMoCap</id>
    <version>1.0.0</version>
    <title>VL.DollarsMoCap</title>
    <authors>vvvv</authors>
    <projectUrl>https://github.com/vvvv/VL.DollarsMoCap</projectUrl>
